{
  "env": {
    "development": {
      "presets": "next/babel",
      "plugins": [["transform-define", "./env-config.js"]]
    },
    "production": {
      "presets": "next/babel",
      "plugins": [["transform-define", "./env-config.js"]]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "next/babel"
      ],
      "plugins": [["transform-define", "./env-config.js"]]
    }
  }
}
