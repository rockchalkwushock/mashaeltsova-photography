{
  "env": {
    "development": {
      "presets": "next/babel",
      "plugins": ["react-intl", ["transform-define", "./env-config.js"]]
    },
    "production": {
      "presets": "next/babel",
      "plugins": [
        [
          "react-intl",
          {
            "messagesDir": "lib/i18n/.messages/"
          }
        ],
        ["transform-define", "./env-config.js"]
      ]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "next/babel"
      ],
      "plugins": ["react-intl"]
    }
  }
}
