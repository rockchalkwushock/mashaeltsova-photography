{
  "name": "mashaeltsova-photography",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "description": "Photography Business Website for Masha Eltsova.",
  "author":
    "Cody Brunner <rockchalkwushock@icloud.com> (https://rcws-development.com)",
  "repository":
    "https://github.com/rockchalkwushock/mashaeltsova-photography.git",
  "homepage":
    "https://github.com/rockchalkwushock/mashaeltsova-photography#readme",
  "bugs": "https://github.com/rockchalkwushock/mashaeltsova-photography/issues",
  "scripts": {
    "cm": "nps commit",
    "dev": "node server.js",
    "now-build": "next build",
    "now-start": "NODE_ENV=production node server.js",
    "prettier": "prettier --single-quote --no-semi --write",
    "prod": "yarn now-build && yarn now-start",
    "start": "nps",
    "test": "nps test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && nps validate.withCoverage"
    }
  },
  "jest": {
    "bail": true,
    "collectCoverageFrom": ["components/**/*.js", "containers/*.js"],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 30
      }
    },
    "setupFiles": ["./jest.setup.js"],
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "testEnvironment": "jsdom"
  },
  "lint-staged": {
    "*.js": ["yarn prettier", "git add"]
  },
  "now": {
    "alias": "mashaeltsovaphotography.com",
    "env": {
      "NODE_ENV": "production"
    },
    "files": [
      "./components",
      "./containers",
      "./i18n",
      "./lib",
      "./pages",
      "./static",
      ".babelrc",
      "env-config.js",
      "next.config.js",
      "package.json",
      "server.js",
      "yarn.lock"
    ],
    "name": "mashaeltsova-photography"
  },
  "dependencies": {
    "accepts": "^1.3.4",
    "axios": "^0.17.0",
    "babel-plugin-transform-define": "^1.3.0",
    "glob": "^7.1.2",
    "intl": "^1.2.5",
    "next": "^4.1.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "raven-js": "^3.19.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-fastclick-alt": "^2.0.0",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.3.5",
    "react-intl": "^2.4.0",
    "react-lazyload": "^2.3.0",
    "react-raven": "^1.2.3",
    "sw-precache-webpack-plugin": "^0.11.4",
    "validator": "^9.1.1"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.2.1",
    "eslint": "^4.10.0",
    "eslint-config-equimper": "^2.2.1",
    "eslint-config-prettier": "^2.7.0",
    "husky": "^0.15.0-beta.13",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "nps": "^5.7.1",
    "nps-utils": "^1.5.0",
    "prettier": "^1.7.3",
    "react-test-renderer": "^16.0.0",
    "webpack-bundle-analyzer": "^2.9.0"
  }
}
